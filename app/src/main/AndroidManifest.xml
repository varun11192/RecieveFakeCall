<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.varunsen.originalcall">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_local_phone_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_local_phone_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.OriginalCall">
        <activity android:name=".CallRecieve"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen"
            ></activity>
        <activity
            android:name=".EnterCallerNameBro"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen"></activity>
    </application>

</manifest>